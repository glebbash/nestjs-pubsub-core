{
  "name": "nestjs-pubsub-core",
  "version": "0.0.0",
  "description": "Nest.js Google PubSub module",
  "keywords": [
    "nestjs",
    "pubsub",
    "typescript"
  ],
  "main": "lib/nestjs-pubsub-core.js",
  "typings": "lib/nestjs-pubsub-core.d.ts",
  "files": [
    "lib"
  ],
  "author": "glebbash <glebbash@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/glebbash/nestjs-pubsub-core"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "lint": "eslint --fix 'src/**/*.ts'",
    "prebuild": "rimraf lib",
    "build": "tsc --project tsconfig.build.json",
    "build:docs": "typedoc --out docs src/nestjs-pubsub-core.ts",
    "test": "jest --config jest.config.ts --coverage",
    "test:prod": "npm run lint && npm run test -- --no-cache"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "eslint --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "peerDependencies": {
    "@google-cloud/pubsub": ">= 2",
    "@nestjs/common": ">= 8"
  },
  "overrides": {
    "uuid": "7.x.x"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@google-cloud/pubsub": "^4.0.6",
    "@nestjs/common": "8.4",
    "@nestjs/core": "8.4",
    "@nestjs/testing": "^8.4.7",
    "@types/jest": "26.0.19",
    "@types/node": "^14.18.21",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^5.2.0",
    "jest": "^28.1.2",
    "jest-config": "26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.3",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typedoc": "^0.23.2",
    "typescript": "^4.7.4"
  }
}
